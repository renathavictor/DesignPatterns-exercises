package factory.model;

public abstract class Usuario {

	private String nome;

	public Usuario() {}

	public Usuario(String nomeo) {
		this.nome = nome;
	}


	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public abstract String saudacao();
	
}
