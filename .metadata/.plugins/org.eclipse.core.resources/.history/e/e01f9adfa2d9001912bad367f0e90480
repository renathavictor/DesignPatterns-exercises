package controller;

import model.Espiao;
import model.Inimigo;

public class InimigoController {
	
	public Inimigo criarEspiao() {
		Inimigo espiao = new Espiao(200, 10, 7, 10);
		return espiao;
	}
	
	public Inimigo criarGuerrilheiro() {
		Inimigo guerrilheiro = new Espiao(100, 5, 8, 8);
		return guerrilheiro;
	}
	
	public Inimigo criarExercito() {
		Inimigo exercito = new Espiao(50, 2, 7, 10);
		return exercito;
	}
	
	

}
